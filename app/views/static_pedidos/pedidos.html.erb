<script>

function PrintElem() {



    var divToPrint = document.getElementById('myDivToPrint');

    var htmlToPrint = '' +
        '<hr>' +
        '<hr>' +
        '<style type="text/css">' +
        'hr, table th, table td {' +
        'border-bottom-style: ' +
        'dotted;'  +
        'border-color:gray;'  +
        'border-width: 1px;' +
        'font-family: "Courier New", Courier, monospace;' +
        'font-size: 70%;' +
        '}' +
        'p.serif {' + 
        'font-family: "Courier New", Courier, monospace;' +
        'font-size: 70%;' +
        '}' + 
        'hr {' +
        'borderstyle: ' +
        'dotted;'  +
        'border-color:gray;'  +
        'border-width: 1px;' +
        '}' +
        '</style>';


        var cliente = '' +
        '<p class="serif">' + 'Cliente: ' + $("#txtNomeCliente").val() + '</p>' +
        '<p class="serif">' + 'Telefone: ' + $("#txtTelefoneCliente").val() + '</p>' +
        '<p class="serif">' + 'Endereço: ' + $("#txtRua").val() + ' Qd. ' + $("#txtQuadra").val() + ' Lt. ' + $("#txtLote").val() + 'Set. ' +  $("#txtBairro").val() + '</p>' +
        '<hr>' +
        '<hr>';

        if ($("#txtNomeCliente").val() != ""){
          htmlToPrint += cliente;
        }

        
    htmlToPrint +=  divToPrint.outerHTML;

    newWin = window.open("");
    
    //$("#txtTelefoneCliente").val("");
    //$("#txtRua").val("");
    //$("#txtQuadra").val("");
    //$("#txtLote").val("");
    //$("#txtBairro").val("");
    //$("#txtReferencia").val("");

    newWin.document.write(htmlToPrint);
    newWin.print();
    newWin.close();
}


function IngredAdicionaMistos(tipo) {

    $("#divAdicionaisMistos").hide();
    $("#h4AdicionaisMistos").hide();  
    $("#divIngredientesMistos").hide();
    $("#h4IngredientesMistos").hide();
    $("#divObsMistos").hide();
    $("#h4ObsMistos").hide();


  if (tipo == "ingredientes") {
    $("#divIngredientesMistos").show();
    $("#h4IngredientesMistos").show();
  } else if (tipo == "adicionais") {
    $("#divAdicionaisMistos").show();
    $("#h4AdicionaisMistos").show();
  } else if ( tipo == "Obs"){
    $("#divObsMistos").show();
    $("#h4ObsMistos").show();
  };

}

function salvaObservacao(desc_observacao){

  $.ajax({
    url: '<%=salva_observacao_path %>',
    data : 
    {
        desc_observacao : desc_observacao
        ,fd_itempedido_id : $("#fd_itempedido_id").val()
    },
    success: function(data){


    }
  }) 

}


function inserirEntrega(){

  $.ajax({
    url: '<%= insere_entrega_path %>',
    data : 
    {
        valr_entrega : $("#valr_entrega").val()
        ,fd_pedido_id : $("#fd_pedido_id").val()
    },
    success: function(data){

      montaComanda($("#fd_pedido_id").val());
      //alert_boot('Pagamento realizado com sucesso.','success','#alert_placeholder_periodo')

    }
  })  
}


function efetuarPagamento(){

  $.ajax({
    url: '<%= efetua_pagamento_path %>',
    data : 
    {
        valr_pedido : $("#total").val()
        ,fd_pedido_id : $("#fd_pedido_id").val()
    },
    success: function(data){

      alert_boot('Pagamento realizado com sucesso.','success','#alert_placeholder_periodo')

    }
  })  
}

function limparcliente(){

        $("#txtNomeCliente").val("");
        $("#txtTelefoneCliente").val("");
        $("#txtRua").val("");
        $("#txtQuadra").val("");
        $("#txtLote").val("");
        $("#txtBairro").val("");
        $("#txtReferencia").val("");

}

function atualizaCliente(fd_cliente_id){

  $.ajax({
    url: '<%= atualiza_cliente_path %>',
    data : 
    {
        fd_cliente_id : fd_cliente_id
        ,fd_pedido_id : $("#fd_pedido_id").val()
    },
    success: function(data){

        for (var i in data) {
          var item = data[i];

          $("#txtNomeCliente").val(item.nome_cliente);
          $("#txtTelefoneCliente").val(item.desc_telefone);
          $("#txtRua").val(item.nome_rua);
          $("#txtQuadra").val(item.numr_quadra);
          $("#txtLote").val(item.numr_lote);
          $("#txtBairro").val(item.nome_bairro);
          $("#txtReferencia").val(item.desc_pontoreferencia);

          alert_boot('Cliente incluído com sucesso.','success','#alert_placeholder_periodo')

        };   

    }
  })  

}


function incluiCliente(){

  $.ajax({
    url: '<%= inclui_cliente_path %>',
    data : 
    {
        nome_cliente : $("#txtNomeCliente").val()
        ,desc_telefone : $("#txtTelefoneCliente").val()
        ,nome_rua : $("#txtRua").val()
        ,numr_quadra : $("#txtQuadra").val()
        ,numr_lote : $("#txtLote").val()
        ,nome_bairro : $("#txtBairro").val()
        ,desc_pontoreferencia : $("#txtReferencia").val()
        ,fd_pedido_id : $("#fd_pedido_id").val()
    },
    success: function(data){

        alert_boot('Cliente incluído com sucesso.','success','#alert_placeholder_periodo')

    }
  })  

}

function buscaClienteTelefone(){


  $.ajax({
    url: '<%= buscaClientetelefone_path %>',
    data : 
    {
        desc_telefone : $("#txtTelefoneCliente").val()
    },
    success: function(data){

        $("div.divClientes").html("");

        for (var i in data) {
          var item = data[i];

            $("div.divClientes").append('<a href="javascript:atualizaCliente('+item.fd_cliente_id+')" class="list-group-item list-group-item-info">' + item.nome_cliente + '<span class="badge">+</span></a>');

        };      


    }
  })  

} 

function buscaCliente(){

  $.ajax({
    url: '<%= buscaCliente_path %>',
    data : 
    {
        nome_cliente : $("#txtNomeCliente").val()
    },
    success: function(data){

        $("div.divClientes").html("");

        for (var i in data) {
          var item = data[i];

            $("div.divClientes").append('<a href="javascript:atualizaCliente('+item.fd_cliente_id+')" class="list-group-item list-group-item-info">' + item.nome_cliente + '<span class="badge">+</span></a>');

        };      


    }
  })  

} 

function escolheTipoAtendimento(tipo_atendimento){

  $.ajax({
    url: '<%= atualiza_tipoatendimento_path %>',
    data : 
    {
        tipo_atendimento : tipo_atendimento
        ,fd_itempedido_id : $("#fd_itempedido_id").val()
    },
    success: function(data){

      $('#mdTipoAtendimento').modal('hide');
      buscaItensPedidos($('#fd_pedido_id').val());

      alert_boot('Atendimento alterado para: '+tipo_atendimento+'.','success','#alert_placeholder_periodo')

    }
  })  

}



function inserePedidoMisto(){

