<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading"><h3 class="panel-title">Listar Cliente</h3></div>

    <table class="table table-hover">
      <thead>
        <tr>
          <th>Nome cliente</th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        <% @fd_clientes.each do |fd_cliente| %>
          <tr>
            <td><%= fd_cliente.nome_cliente %></td>

          <td align='right'>
            <div class="btn-group">
              <%= link_to fd_cliente, :class => "btn btn-default btn-sm" do %>
                <span class="glyphicon glyphicon-list-alt"></span> Exibir
              <% end %>             
              <%= link_to edit_fd_cliente_path(fd_cliente), :class => "btn btn-default btn-sm" do %>
                <span class="glyphicon glyphicon-pencil"></span> Editar
              <% end %>             
              <%= link_to fd_cliente , :method => :delete,  :confirm => 'Deseja excluir o registro?', :class => "btn btn-default btn-sm" do %>
                <span class="glyphicon glyphicon-trash"></span> Excluir
              <% end %>            
            </div>
          </td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>

<div>
   <%= will_paginate @collection, renderer: BootstrapPagination::Rails, :inner_window => 2 %>
</div>




<%= link_to new_fd_cliente_path, :class => "btn btn-default btn-sm" do %>
     <span class="glyphicon glyphicon-file"></span>Novo
<% end %>
