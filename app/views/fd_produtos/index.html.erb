<h1>Listing fd_produtos</h1>

<table>
  <thead>
    <tr>
      <th>Nome produto</th>
      <th>Desc produto</th>
      <th>Numr porcentagem</th>
      <th>Fd empresa</th>
      <th>Fd categoriaproduto</th>
      <th>Fd usuario</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @fd_produtos.each do |fd_produto| %>
      <tr>
        <td><%= fd_produto.nome_produto %></td>
        <td><%= fd_produto.desc_produto %></td>
        <td><%= fd_produto.numr_porcentagem %></td>
        <td><%= fd_produto.fd_empresa %></td>
        <td><%= fd_produto.fd_categoriaproduto %></td>
        <td><%= fd_produto.fd_usuario %></td>
        <td><%= link_to 'Show', fd_produto %></td>
        <td><%= link_to 'Edit', edit_fd_produto_path(fd_produto) %></td>
        <td><%= link_to 'Destroy', fd_produto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fd produto', new_fd_produto_path %>
